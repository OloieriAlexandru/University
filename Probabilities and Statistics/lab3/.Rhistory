ex1 = function(N){
cnt = 0;
for (i in 1:N){
x = runif(1,-1,1);
y = runif(1,-1,1);
z = runif(1,-1,1);
if (x*x + y*y + z*z <= 1){
cnt = cnt + 1;
}
}
return (8*cnt/N);
}
ex1(100000);
