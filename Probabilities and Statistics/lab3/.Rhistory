rezex1() = function(N){
cnt = 0;
for (i in 1:N){
x = runif(1,-1,1);
y = runif(1,-1,1);
if (x*x + y*y <= 1){
cnt = cnt + 1;
}
}
return (4*cnt/N);
}
rezex1(10000);
