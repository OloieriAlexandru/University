outliers_iqr = function(x)
{
data = quantile(x);
iqr = data[[4]] - data[[2]];
altX = vector();
j = 0;
for (i in 1:length(x))
{
if (!(x[i]>=data[[2]] - 1.5*iqr & x[i]<=data[[4]] + 1.5*iqr)){
j = j+1;
altX[j] = x[i];
}
}
return (altX);
}
